<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="HUB2" />
		<Option pch_mode="2" />
		<Option compiler="sdcc" />
		<Build>
			<Target title="Release">
				<Option output="bin/Release/HUB2" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="sdcc" />
				<Compiler>
					<Add option="--out-fmt-ihx" />
					<Add option="--no-std-crt0" />
					<Add option="-mz80" />
					<Add option="--opt-code-speed" />
					<Add option="--verbose" />
					<Add option="--code-loc 0x180 --data-loc 0x0 --disable-warning 196  --allow-unsafe-read" />
