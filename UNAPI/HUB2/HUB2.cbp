					<Add library="../fusion-c/lib/asm.lib" />
					<Add library="../fusion-c/lib/base64.lib" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c ren bin\Release\HUB2.exe HUB2.ihx" />
