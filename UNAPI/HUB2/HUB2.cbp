			<Option weight="0" />
		</Unit>
		<Unit filename="src/hget.h" />
		<Unit filename="src/hgetlib.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
